{
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "import",
    "react"
  ],
  "preset": [
    "airbnb",
    "react"
  ],
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-else-return": 0,
    "no-console": 0
  }
}